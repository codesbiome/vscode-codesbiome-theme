  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "comment.block.html",
        "punctuation.definition.comment",
        "punctuation.definition.comment.html"
      ],
      "settings": {
        "foreground": "#676773"
      }
    },
    {
      "name": "Documentation",
      "scope": ["keyword.other.phpdoc.php"],
      "settings": {
        "foreground": "#9595A6"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "entity.name.type.class.php",
        "keyword.operator.class.php",
        "string.unquoted.plain.out.yaml"
      ],
      "settings": {
        "foreground": "#c9c9d1"
      }
    },
    {
      "name": "Variables [2]",
      "scope": ["variable.scss"],
      "settings": {
        "foreground": "#3eafa5"
      }
    },
    {
      "name": "Dim Variables / Properties",
      "scope": ["support.type.vendored.property-name.css"],
      "settings": {
        "foreground": "#737373"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#c9c9d1"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#e68091"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#B58DC4"
      }
    },
    {
      "name": "Other storage type",
      "scope": [
        "storage.type.php",
        "storage.type.cs",
        "punctuation.section.embedded",
        "support.class.php",
        "entity.name.scope-resolution.cpp"
      ],
      "settings": {
        "foreground": "#E0957B"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant.other.color",
        "punctuation.definition.tag",
        "keyword.other.template",
        "keyword.other.substitution",
        "punctuation.definition.variable.php",
        "constant.other.class.php",
        "meta.function-call.php",
        "support.function",
        "keyword.other",
        "keyword.operator",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.definition.tag.begin.svelte",
        "punctuation.definition.tag.end.svelte"
      ],
      "settings": {
        "foreground": "#B58DC4"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag.sgml", "markup.deleted.git_gutter"],
      "settings": {
        "foreground": "#B58DC4"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "keyword.other.special-method",
        "support.function.magic.php"
      ],
      "settings": {
        "foreground": "#C7A65D"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#c9c9d1"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#e68091"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "entity.other.attribute-name.pseudo-class.css",
        "punctuation.definition.list.begin.markdown",
        "variable.other.readwrite.ts",
        "variable.other.readwrite.cs"
      ],
      "settings": {
        "foreground": "#7bbee0e3"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#ec8383"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#62A362"
      }
    },
    {
      "name": "Class, Support",
      "scope": ["support.type", "markup.changed.git_gutter", "support.type.sys-types"],
      "settings": {
        "foreground": "#C7A65D"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#c9c9d1"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#c9c9d1"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js",
        "variable.other.constant.property.js",
        "variable.other.constant.property.ts"
      ],
      "settings": {
        "foreground": "#f78c6cc9"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#B58DC4"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "meta.method.js",
      "scope": ["meta.class-method.js entity.name.function.js", "variable.function.constructor"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#C7A65D"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#C7A65D"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#E0957B"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#ff5370e8"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": ["source.js constant.other.object.key.js string.unquoted.label.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ff5370e8"
      }
    },
